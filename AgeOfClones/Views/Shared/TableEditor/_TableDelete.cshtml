@model object

@{ 
    var data = ViewData["Data"] as Dictionary<string,string>;
}

<h3>Are you sure you want to delete @ViewData["Name"]?</h3>
<div>
    <form asp-action="@ViewData["Action"]">
        @if (data != null)
        {
            foreach(var item in data)
            {
                <input type="hidden" id="@item.Key" name="@item.Key" value="@item.Value" />

            }

        }


        <input type="submit" value="Delete" class="btn btn-default" />
    </form>
</div>