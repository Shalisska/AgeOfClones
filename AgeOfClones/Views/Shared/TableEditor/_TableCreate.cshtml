@using AgeOfClones.Areas.Management.Models

@model IEnumerable<EntityEditorCurrentFieldModel>

<form asp-action="@ViewData["Action"]">
    <table>
        @foreach(var item in Model)
        {
            <tr>
                <td>
                    @item.PropertyName
                </td>
                <td>
                    @{ Html.RenderPartial("TableEditor/_TableEditorCell", item); }
                </td>
            </tr>

        }
    </table>

    <div class="form-group">
        <input type="submit" value="Create" class="btn btn-default" />
    </div>
</form>