@using Application.Models.TableEditor

@model TableEditorModel

<form asp-action="" asp-antiforgery="true">
    <table>
        <thead>
            <tr>
                <th>
                    <input type="checkbox" id="check-all" />
                </th>

                @foreach (var column in Model.Columns)
                {
                    <th>
                        @column.Name
                    </th>

                }
            </tr>
        </thead>
        <tbody>
            @{Html.RenderPartial("TableEditor/_TableEditorRow", Model.GetTableRows());}
        </tbody>
    </table>
</form>
